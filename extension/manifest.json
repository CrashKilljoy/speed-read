{
  "manifest_version": 2,
  "name": "Speed read",
  "description": "Read selected text really fast. The icon is based on works  Gregor Cresnar and Icons Bazaar from the Noun Project.",
  "version": "1.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "web_accessible_resources": [
    "dist/index.html",
    "fontello.woff2"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "applications":{
    "gecko":{
      "id": "@squirt-jpm"
    }
  }
}
